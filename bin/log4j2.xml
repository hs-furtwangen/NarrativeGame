<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout
				pattern="%d{HH:mm:ss.SSS} [%t] [%-5level] %logger{36} - %msg%n">
			</PatternLayout>
		</Console>
		<File name="Logfile" fileName="./main.log">
			<PatternLayout
				pattern="%d{HH:mm:ss.SSS} [%t] [%-5level] %logger{36} - %msg%n">
			</PatternLayout>
		</File>
	</Appenders>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="Logfile" />
		</Root>
	</Loggers>
</Configuration>